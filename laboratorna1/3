'use strict';

const array = [false, 'JavaScript', 99, -7, 0, true, 'YouTube', 3.1415, 'Steam', -1000, 'minecraft', false];
console.log('Array:', array);

const typesCount = {};

for (const el of array) {
  const type = typeof el;       
  if (!(type in typesCount)) {  
    typesCount[type] = 0;        
  }
  typesCount[type]++;           
}

console.log('Types count:', typesCount);